    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "pytest",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "args": [
                "--pdb"
            ],
            "justMyCode": true
        },
        {
            "name": "torchinstaller",
            "type": "python",
            "request": "launch",
            "module": "torchinstaller",
            "console": "integratedTerminal",
            "args": [
                "-pt",  
                "-c",
                "cu116",
                "-l",
                "-pyg",
                "-u",
                "pip"
            ],
            // "args": [ "-d" ],
            "justMyCode": true
        }
    ]
}